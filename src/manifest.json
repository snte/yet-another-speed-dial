{
  "manifest_version": 2,
  "name": "yet another speed dial",
   "description": "A modern speed dial for Firefox and Chrome.",
  "version": "1.5.2",
  "author": "dev@conceptualspace.net",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "64": "icons/icon64.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "storage",
    "unlimitedStorage",
    "activeTab",
    "tabs",
    "<all_urls>"
  ],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "background": {
    "scripts": [
      "js/browser-polyfill.min.js",
      "js/background.js"
    ]
  }
}
